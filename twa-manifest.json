{
  "packageId": "app.nostria.twa",
  "host": "nostria.app",
  "name": "Nostria",
  "launcherName": "Nostria",
  "display": "standalone",
  "themeColor": "#5953A9",
  "themeColorDark": "#000000",
  "navigationColor": "#000000",
  "navigationColorDark": "#000000",
  "navigationDividerColor": "#000000",
  "navigationDividerColorDark": "#000000",
  "backgroundColor": "#303030",
  "enableNotifications": true,
  "startUrl": "/",
  "iconUrl": "https://nostria.app/icons/icon-512x512.png",
  "splashScreenFadeOutDuration": 300,
  "signingKey": {
    "path": "/workspace/android.keystore",
    "alias": "KEY_ALIAS"
  },
  "appVersionName": "4",
  "appVersionCode": 4,
  "shortcuts": [
    {
      "name": "Profile",
      "shortName": "Profile",
      "url": "https://nostria.app/p",
      "chosenIconUrl": "https://nostria.app/icons/icon-128x128.png"
    },
    {
      "name": "Notifications",
      "shortName": "Notifications",
      "url": "https://nostria.app/notifications",
      "chosenIconUrl": "https://nostria.app/icons/icon-128x128.png"
    },
    {
      "name": "Settings",
      "shortName": "Settings",
      "url": "https://nostria.app/settings",
      "chosenIconUrl": "https://nostria.app/icons/icon-128x128.png"
    }
  ],
  "generatorApp": "bubblewrap-cli",
  "webManifestUrl": "https://nostria.app/manifest.webmanifest",
  "fallbackType": "customtabs",
  "features": {
    "playBilling": {
      "enabled": true
    }
  },
  "alphaDependencies": {
    "enabled": false
  },
  "enableSiteSettingsShortcut": true,
  "isChromeOSOnly": false,
  "isMetaQuest": false,
  "fullScopeUrl": "https://nostria.app/",
  "minSdkVersion": 21,
  "orientation": "any",
  "fingerprints": [],
  "additionalTrustedOrigins": [],
  "retainedBundles": [],
  "protocolHandlers": [
    {
      "protocol": "web+nostr",
      "url": "https://nostria.app/?nostr=%s"
    },
    {
      "protocol": "nostr",
      "url": "https://nostria.app/?nostr=%s"
    }
  ],
  "fileHandlers": [],
  "shareTarget": {
    "action": "https://nostria.app/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": [
            "image/*",
            "video/*",
            "audio/*",
            "text/*",
            "application/pdf"
          ]
        }
      ]
    }
  },
  "launchHandlerClientMode": [
    "focus-existing",
    "auto"
  ],
  "appVersion": "4"
}