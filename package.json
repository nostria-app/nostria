{
  "name": "nostria",
  "version": "0.1.7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build --configuration=development",
    "serve:ssr": "npm run build:ssr && npm run serve:ssr:app",
    "build:ssr:prod": "ng build --configuration=production",
    "serve:ssr:prod": "npm run build:ssr:prod && npm run serve:ssr:app",
    "watch": "ng build --watch --configuration development",
    "gen:api": "ng-openapi-gen --input http://localhost:3000/openapi.json --output src/app/api --module false",
    "gen:i18n": "ng extract-i18n",
    "test": "ng test",
    "tauri": "tauri",
    "get-version": "node -p \"require('./package.json').version\"",
    "serve:ssr:app": "node dist/app/server/server.mjs",
    "debug:ssr:build": "ng build && ng run app:serve",
    "debug:ssr:serve": "node --inspect dist/app/server/server.mjs",
    "debug:ssr": "npm run debug:ssr:build && npm run debug:ssr:serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.3",
    "@angular/cdk": "^20.0.3",
    "@angular/common": "^20.0.3",
    "@angular/compiler": "^20.0.3",
    "@angular/core": "^20.0.3",
    "@angular/forms": "^20.0.3",
    "@angular/material": "^20.0.3",
    "@angular/platform-browser": "^20.0.3",
    "@angular/platform-browser-dynamic": "^20.0.3",
    "@angular/platform-server": "^20.0.3",
    "@angular/router": "^20.0.3",
    "@angular/service-worker": "^20.0.3",
    "@angular/ssr": "^20.0.2",
    "@getalby/sdk": "^5.1.1",
    "@noble/hashes": "^1.8.0",
    "@progress/jszip-esm": "^1.0.4",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "blurhash": "^2.0.5",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "idb": "^8.0.3",
    "marked": "^15.0.12",
    "nostr-tools": "^2.15.0",
    "qr": "^0.5.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@angular/build": "^20.0.2",
    "@angular/cli": "^20.0.2",
    "@angular/compiler-cli": "^20.0.3",
    "@angular/localize": "^20.0.3",
    "@tauri-apps/cli": "^2",
    "@types/express": "^5.0.3",
    "@types/jasmine": "~5.1.8",
    "@types/ws": "^8.18.1",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-openapi-gen": "^0.53.0",
    "typescript": "~5.8.3"
  }
}
